# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:5000/api

# Google OAuth Configuration (Frontend)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=your_google_expo_client_id

# App Configuration
EXPO_PUBLIC_APP_NAME="Smart Study Assistant"
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment
NODE_ENV=development
EXPO_PUBLIC_ENV=development

# Payment Configuration (Frontend URLs)
EXPO_PUBLIC_PAYMENT_SUCCESS_URL=http://localhost:3000/payment/success
EXPO_PUBLIC_PAYMENT_CANCEL_URL=http://localhost:3000/payment/cancel

# Voice Input Configuration
EXPO_PUBLIC_VOICE_LANGUAGE=vi-VN
EXPO_PUBLIC_VOICE_ENABLED=true

# Debug Configuration
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug
